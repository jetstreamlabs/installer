{
  "name": "serenity/installer",
  "description": "Serenity application installer.",
  "keywords": [
    "serenity"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Taylor Otwell",
      "email": "taylor@laravel.com"
    },
    {
      "name": "secondman",
      "email": "secondman@jetstreamlabs.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/console": "^4.0|^5.0|^6.0",
    "symfony/process": "^4.2|^5.0|^6.0"
  },
  "require-dev": {
    "jetstreamlabs/pinte": "^1.0",
    "phpunit/phpunit": "^8.0|^9.3"
  },
  "bin": [
    "bin/serenity"
  ],
  "autoload": {
    "psr-4": {
      "Serenity\\Installer\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Serenity\\Installer\\Console\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
